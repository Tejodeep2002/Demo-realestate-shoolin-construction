{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"next",
		"plugin:react/recommended",
		"next/core-web-vitals",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"no-console": "warn"
	}
}
